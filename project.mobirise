        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)"
            },
            ".content": {
              "position": "relative",
              "display": "flex",
              "flex-direction": "column",
              "align-items": "center"
            },
            ".mbr-section-title": {
              "color": "#232323",
              "margin-bottom": "20.4px"
            },
            ".animation-box": {
              "position": "relative",
              "width": "78px",
              "height": "86px"
            },
            "@keyframes zoom-1": {
              "0%, 10%": {
                "transform": "scale(0) rotate(0)"
              },
              "30%, 32%": {
                "transform": "scale(0.6) rotate(10deg)"
              },
              "100%, 85%": {
                "transform": "scale(0) rotate(60deg)"
              }
            },
            "@keyframes zoom-2": {
              "0%, 15%": {
                "transform": "scale(0) rotate(0)"
              },
              "50%, 54%": {
                "transform": "scale(0.4) rotate(0)"
              },
              "100%, 82%": {
                "transform": "scale(0) rotate(80deg)"
              }
            },
            ".star": {
              "color": "@animationColor",
              "fill": "currentColor",
              "overflow": "hidden"
            },
            ".star:nth-of-type(2)": {
              "position": "absolute",
              "top": "-35px",
              "left": "50px",
              "transform": "scale(0)",
              "& when (@animation)": {
                "animation": "zoom-1 3.1s infinite"
              }
            },
            ".star:nth-of-type(3)": {
              "position": "absolute",
              "top": "-45px",
              "left": "-50px",
              "& when (@animation)": {
                "animation": "zoom-2 3.1s infinite"
              }
            }
          },
